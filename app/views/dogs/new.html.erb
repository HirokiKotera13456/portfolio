<h1>画像投稿フォーム</h1>
<%= form_with model: @newdog, local:true do |f| %>
  <div class="form-group py-3">
  <%= f.label :画像 %>
  <%= f.attachment_field :image, class:"form-control" %>
</div>
  <h4>愛犬の名前</h4>
  <%= f.text_field :shop_name %>
  <h4>愛犬の説明</h4>
  <%= f.text_area :caption %>
  <%= f.submit '投稿' %>
<% end %>