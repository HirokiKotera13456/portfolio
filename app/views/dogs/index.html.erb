

<% @dogs.each do |dog| %>
  <div>
  <%= link_to dog_path(dog.id) do %>
    <%= attachment_image_tag dog, :image, size: "300x300" %>
  <% end %>
    <p>ショップ名：<%= dog.name %></p>
    <p>説明：<%= dog.caption %></p>
    <p>ユーザーネーム：<%= dog.user.name %></p>
    <%= link_to "投稿ページへ" ,dog_path(dog.id) %>
  </div>
  <% end %>